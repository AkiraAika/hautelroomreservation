<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".ActivityBooked"></activity>
        <activity android:name=".ActivityReceipt" />
        <activity android:name=".ActivityCheckout" />
        <activity android:name=".ActivityBooking"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".ActivityUpdatePassword" />
        <activity android:name=".ActivityUpdateProfile" />
        <activity android:name=".ClassUserProfile" />
        <activity android:name=".ActivityProfile" />
        <activity android:name=".ActivityResetPassword" />
        <activity android:name=".ActivityRegistration" />
        <activity android:name=".ActivityHome" />
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>